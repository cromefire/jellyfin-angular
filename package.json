{
    "name": "jellyfin-angular",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "yarn prebuild && ng serve",
        "build": "yarn prebuild && ng build --aot --prod --source-map",
        "build:dev": "yarn prebuild && ng build",
        "build:gh-pages": "yarn prebuild && ng build --aot --prod --source-map --base-href /jellyfin-angular/ --output docs",
        "test": "yarn prebuild && ng test",
        "lint": "ng lint",
        "e2e": "yarn prebuild && ng e2e",
        "prebuild": "node ./scripts/pre-build.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.1.2",
        "@angular/cdk": "~8.1.2",
        "@angular/common": "~8.1.2",
        "@angular/compiler": "~8.1.2",
        "@angular/core": "~8.1.2",
        "@angular/forms": "~8.1.2",
        "@angular/material": "^8.1.2",
        "@angular/platform-browser": "~8.1.2",
        "@angular/platform-browser-dynamic": "~8.1.2",
        "@angular/pwa": "^0.802.0",
        "@angular/router": "~8.1.2",
        "@angular/service-worker": "~8.1.2",
        "hammerjs": "^2.0.8",
        "ngx-infinite-scroll": "^8.0.0",
        "prettier": "^1.18.2",
        "rxjs": "~6.4.0",
        "tslib": "^1.9.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.801.2",
